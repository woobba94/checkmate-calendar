{
  "name": "checkmate-calendar",
  "private": true,
  "version": "1.0.0",
  "description": "Checkmate Calendar - Monorepo",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "dev:landing": "npm run dev --workspace=projects/landing",
    "dev:calendar": "npm run dev --workspace=projects/calendar",
    "build:landing": "npm run build --workspace=projects/landing",
    "build:calendar": "npm run build --workspace=projects/calendar",
    "preview:landing": "npm run preview --workspace=projects/landing",
    "preview:calendar": "npm run preview --workspace=projects/calendar"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
